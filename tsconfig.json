{
	"compilerOptions": {
		"outDir": "./built/",
		"sourceMap": true,
		"strict": true,
		"noImplicitReturns": true,
		"module": "es2015",
		"moduleResolution": "node",
		"target": "es5"
	},
	"include": [
		"*"
	],
	"paths": {
		"baseUrl": ".",
		"@/*": ["./*"]
	},
	"exclude": ["node_modules", "Dist"]
}
